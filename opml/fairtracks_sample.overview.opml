<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <body>
    <outline _name="#toplevel" ancestors="." augmented="." comments="." const="." constIf="." default="." description="." enum="." examples="." foreignProperty="." format="." matchType="." minItems="0" namespace="." ontology="." ontologyTermPair="." pattern="." ref="." requireAnyOf="." requireIf="." required="." title="Sample" type="object" unique="." />
    <outline _name="#version_url" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v$breaking/$breaking.$feature.$fix/json/schema/fairtracks_sample.schema.json" title="Version URL" />
    <outline _name="@schema" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v$breaking/current/json/schema/fairtracks_sample.schema.json" description="The absolute URL of the 'current' version of the relevant FAIRtracks JSON schema within the same major version as the JSON document follows (which should ensure compatibility). Must match the value of '$id' in the linked schema" format="uri" pattern="^(https?|ftp)://" required="false" title="Version URL" type="string" />
    <outline _name="global_id" description="Global sample identifier, resolvable by identifiers.org" examples="biosample:SAMN01731491|biosample:SAMN01731491" format="curie" matchType="canonical" namespace="biosample" required="false" title="Global ID" type="string" />
    <outline _name="local_id" description="Submitter-local identifier (within investigation/hub) for sample (in CURIE-format, if applicable)" examples="encode:ENCBS192PUU|encode:ENCBS967QBI" required="true" title="Local ID" type="string" unique="true" />
    <outline _name="species_id" description="Species identifier, resolvable by identifiers.org" examples="taxonomy:9606|taxonomy:9606" format="curie" matchType="canonical" namespace="taxonomy" required="true" title="Species ID" type="string" />
    <outline _name="species_name" augmented="true" description="Species name according to the NCBI Taxonomy database (https://www.ncbi.nlm.nih.gov/taxonomy)" examples="Homo sapiens|Homo sapiens" required="false" title="Species Name" type="string" />
    <outline _name="biospecimen_class" description="Main type of structural unit to be used for classification of the sample" ontologyTermPair="id=0/term_id;label=0/term_label" required="true" title="Biospecimen Class" type="object">
      <outline _name="term_id" description="URL linking to an ontology term" enum="http://purl.obolibrary.org/obo/NCIT_C12508|http://purl.obolibrary.org/obo/NCIT_C12913|http://purl.obolibrary.org/obo/NCIT_C16403|http://purl.obolibrary.org/obo/NCIT_C103199" examples="http://purl.obolibrary.org/obo/NCIT_C12508|http://purl.obolibrary.org/obo/NCIT_C12508" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/ncit.owl" pattern="^(https?|ftp)://" required="true" title="Term ID" type="string" />
      <outline _name="term_label" augmented="true" description="Exact value according to the ontology used" enum="Cell|Abnormal Cell|Cell Line|Organism Part" examples="Cell|Cell" required="false" title="Term Label" type="string" />
    </outline>
    <outline _name="sample_type" description="Main classification of the sample" required="true" title="Sample Type" type="object">
      <outline _name="cell_type" comments="requireIf: Cell" description="Cell type of isolated normal cells in the sample. This property should only be used if biospecimen_class is set to &quot;Cell&quot;." ontologyTermPair="id=0/term_id;label=0/term_label" requireIf="2/biospecimen_class/term_id=http://purl.obolibrary.org/obo/NCIT_C12508" required="false" title="Cell Type (Normal)" type="object">
        <outline _name="term_id" ancestors="http://purl.obolibrary.org/obo/CL_0000003" description="URL linking to an ontology term" examples="http://purl.obolibrary.org/obo/CL_0000236|http://purl.obolibrary.org/obo/CL_0000236" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/cl.owl" pattern="^(https?|ftp)://" required="true" title="Term ID" type="string" />
        <outline _name="term_label" augmented="true" description="Exact value according to the ontology used" examples="B cell|B cell" required="false" title="Term Label" type="string" />
      </outline>
      <outline _name="abnormal_cell_type" comments="requireIf: Abnormal Cell" description="Cell type of isolated abnormal cells in the sample. This property should only be used if biospecimen_class is set to &quot;Abnormal Cell&quot;." ontologyTermPair="id=0/term_id;label=0/term_label" requireIf="2/biospecimen_class/term_id=http://purl.obolibrary.org/obo/NCIT_C12913" required="false" title="Cell Type (Abnormal)" type="object">
        <outline _name="term_id" ancestors="http://purl.obolibrary.org/obo/NCIT_C12913" description="URL linking to an ontology term" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/ncit.owl" pattern="^(https?|ftp)://" required="true" title="Term ID" type="string" />
        <outline _name="term_label" augmented="true" description="Exact value according to the ontology used" required="false" title="Term Label" type="string" />
      </outline>
      <outline _name="cell_line" comments="requireIf: Cell Line" description="Cultured cell line used in the sample. This property should only be used if biospecimen_class is set to &quot;Cell Line&quot;." ontologyTermPair="id=0/term_id;label=0/term_label" requireIf="2/biospecimen_class/term_id=http://purl.obolibrary.org/obo/NCIT_C16403" required="false" title="Cell Line" type="object">
        <outline _name="term_id" ancestors="http://purl.obolibrary.org/obo/CL_0000010" description="URL linking to an ontology term" format="term" matchType="exact" ontology="http://www.ebi.ac.uk/efo/efo.owl" pattern="^(https?|ftp)://" required="true" title="Term ID" type="string" />
        <outline _name="term_label" augmented="true" description="Exact value according to the ontology used" required="false" title="Term Label" type="string" />
      </outline>
      <outline _name="organism_part" comments="requireIf: Organism Part" description="Part of organism (typically tissue or organ) from which the sample was taken, or cell line was derived from. This property  must be used is biospecimen_class is set to &quot;Organism Part&quot;, but can also be used for the other values of biospecimen_class." ontologyTermPair="id=0/term_id;label=0/term_label" requireIf="2/biospecimen_class/term_id=http://purl.obolibrary.org/obo/NCIT_C103199" required="false" title="Organism Part (Tissue/Organ)" type="object">
        <outline _name="term_id" ancestors="http://purl.obolibrary.org/obo/UBERON_0000465" description="URL linking to an ontology term" examples="http://purl.obolibrary.org/obo/UBERON_0000926|http://purl.obolibrary.org/obo/UBERON_0000926" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/uberon.owl" pattern="^(https?|ftp)://" required="true" title="Term ID" type="string" />
        <outline _name="term_label" augmented="true" description="Exact value according to the ontology used" examples="mesoderm|mesoderm" required="false" title="Term Label" type="string" />
      </outline>
      <outline _name="details" description="Important details about the sample classification (to be included in the 'sample_type/summary' property)" examples="CD20+|CD20+" required="false" title="Sample Type Details" type="string" />
      <outline _name="summary" augmented="true" description="Main classification of the sample. Summary of 'sample_type' sub-properties: 'cell_type', 'abnormal_cell_type', 'cell_line', or 'organism_part' (and adding 'details'), according to 'biospecimen_class'" examples="B cell (mesoderm, CD20+)|B cell (mesoderm, CD20+)" required="false" title="Sample Type (Summary)" type="string" />
    </outline>
    <outline _name="phenotype" description="Main phenotype (e.g. disease) connected to the sample" ontologyTermPair="id=0/term_id;label=0/term_label" ref="fairtracks_phenotype.schema.json" required="true" title="Phenotype" type="object" />
  </body>
</opml>
