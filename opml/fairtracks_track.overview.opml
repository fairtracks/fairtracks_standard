<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <body>
    <outline _name="#toplevel" ancestors="." autogenerated="." comments="." const="." constIf="." default="." description="." enum="." examples=".|.|.|." foreignProperty="." format="." matchType="." minItems="0" namespace="." ontology="." ontologyTermPair="." pattern="." ref="." requireAnyOf="." requireIf="." required="." title="Track" type="object" unique="." />
    <outline _name="@schema" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v1/current/json/schema/fairtracks_track.schema.json" description="The JSON Schema absolute URL. Used to link JSON data to a JSON schema. Must match the value of '$id' in the linked schema" format="uri" pattern="^(https?|ftp)://" required="false" type="string" />
    <outline _name="global_id" autogenerated="true" comments="curie and matchType to be added later" description="Global track identifier, resolvable by identifiers.org [to be created by us]" examples="fairtracks:1|fairtracks:2|fairtracks:3|fairtracks:4" namespace="fairtracks" required="false" type="string" />
    <outline _name="local_id" description="Submitter-local identifier (within investigation/hub) for study (in CURIE-format, if applicable)" examples="encode:ENCFF625ZYB|encode:ENCFF718EPO|encode:ENCFF717PIO|encode:ENCFF955LOC" required="true" type="string" unique="true" />
    <outline _name="assembly_id" description="Genome assembly identifier, resolvable by identifiers.org. Tracks should be annotated with the lowest version of the reference genome that contains all the sequences referenced by the track. Also, GCF (Refseq) ids should be preferred to GCA (Genbank) ids" examples="insdc.gca:GCF_000001405.26|insdc.gca:GCF_000001405.26|insdc.gca:GCF_000001405.26|insdc.gca:GCF_000001405.26" format="curie" matchType="canonical" namespace="insdc.gca" required="true" type="string" />
    <outline _name="assembly_name" description="Genome assembly name or synonym, according to the NCBI Assembly database. For tracks following UCSC-style chromosome names (e.g., &quot;chr1&quot;), the UCSC synonym should be used instead of the official name" examples="hg38|hg38|hg38|hg38" required="true" type="string" />
    <outline _name="experiment_ref" description="Reference to the experiment of the track (using the submitter-local identifier of the sample)" examples="encode:ENCSR000DQP|encode:ENCSR000DQP|encode:ENCSR000DQP|encode:ENCSR000DQP" foreignProperty="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v1/1.0/json/schema/fairtracks_experiment.schema.json#local_id" format="foreign_ref" required="true" type="string" />
    <outline _name="raw_file_ids" minItems="1" required="false" type="array">
      <outline description="List of identifiers to raw data files used to create track (typically BAM), resolvable by identifiers.org" examples="encode: ENCFF207FGO;encode:ENCFF842FEQ|encode:ENCFF207FGO;encode:ENCFF842FEQ|encode:ENCFF207FGO;encode:ENCFF842FEQ|encode:ENCFF207FGO;encode:ENCFF842FEQ" format="curie" matchType="canonical" required="true" type="string" />
    </outline>
    <outline _name="file_url" description="A URL to the track data file" examples="https://www.encodeproject.org/files/ENCFF625ZYB/@@download/ENCFF625ZYB.bed.gz|https://www.encodeproject.org/files/ENCFF718EPO/@@download/ENCFF718EPO.bigBed|https://www.encodeproject.org/files/ENCFF717PIO/@@download/ENCFF717PIO.bigWig|https://www.encodeproject.org/files/ENCFF955LOC/@@download/ENCFF955LOC.bigWig" format="uri" pattern="^(https?|ftp|rsync)://" required="true" type="string" />
    <outline _name="file_name" autogenerated="true" description="Name of the track file" examples="r1.narrowPeak.gz|r1.narrowPeak.bb|r1.fc_signal.bw|r1.pvalue_signal.bw" required="false" type="string" />
    <outline _name="label_short" description="A short label of the track file. Suggested maximum length is 17 characters" examples="ENCFF625ZYB|ENCFF718EPO|ENCFF717PIO| ENCFF955LOC" required="true" type="string" />
    <outline _name="label_long" description="A long label of the track file. Suggested maximum length is 80 characters" examples="H3K4me3 ChIP-seq of B cell peaks rep1 ENCSR000DQP - ENCFF625ZYB|H3K4me3 ChIP-seq of B cell peaks rep1 ENCSR000DQP - ENCFF718EPO|H3K4me3 ChIP-seq of B cell fold change over control rep1 ENCSR000DQP - ENCFF717PIO|H3K4me3 ChIP-seq of B cell signal p-value rep1 ENCSR000DQP - ENCFF955LOC" required="true" type="string" />
    <outline _name="file_format" description="File format of the track data file" ontologyTermPair="id=0/term_id;label=0/term_label" required="true" type="object">
      <outline _name="term_id" description="URL linking to an ontology term" examples="http://edamontology.org/format_3613|http://edamontology.org/format_3004|http://edamontology.org/format_3006|http://edamontology.org/format_3006" format="term" matchType="exact" ontology="http://edamontology.org/EDAM.owl" pattern="^(https?|ftp)://" required="true" type="string" />
      <outline _name="term_label" autogenerated="true" description="Exact value according to the ontology used" examples="ENCODE narrow peak format|bigBed|bigWig|bigWig" required="false" type="string" />
    </outline>
    <outline _name="type_of_condensed_data" description="Type of condensed track data: Track data, by definition, is formed downstream of some data condensation process. However, the condensed data vary in form and content, technically speaking, and thus in their interpretation. Still, there is a limited set of common types of condensed track data which are able to describe the vast majority of track files" enum="Sequence-derived regions|Experimentally-derived regions|Predicted regions|Predicted segmentation|Population-derived variants|Individual variants|Peaks|Broad peaks|Narrow peaks|Gapped peaks|Signal values (fold change)|Signal values (p-value)|Signal values (log likelihood)|Signal values (other)|Read coverage|Read counts|Mapped single-end reads|Mapped paired-end reads|Other" examples="Narrow peaks|Narrow peaks|Signal values (fold change)|Signal values (p-value)" required="true" type="string" />
    <outline _name="geometric_track_type" description="Geometric type of track, according to the delineation of tracks into one of fifteen logical track types based upon their core informational properties (see doi:10.1186/1471-2105-12-494) " enum="Points|Valued points|Segments|Valued segments|Genome partition|Step function|Function|Linked points|Linked valued points|Linked segments|Linked valued segments|Linked genome partition|Linked step function|Linked function|Linked base pairs" examples="Segments|Segments|Step function|Step function" required="true" type="string" />
    <outline _name="checksum" required="true" type="object">
      <outline _name="cs_method" description="Method of checksum generation" enum="MD5|SHA-256" examples="MD5|MD5|MD5|MD5" required="true" type="string" />
      <outline _name="cs_hash" description="Checksum of track file, using the method described in cs_method" examples="0ab98dbd2a2193ab3e09df030ea4f934|6d2c6c99a7407f1c49ab163e41d9b575|550ae974bf94f1deb1676613ab24a5da|942ea96e86313d4338cf3fd020882c4d" required="true" type="string" />
    </outline>
  </body>
</opml>
