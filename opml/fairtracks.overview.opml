<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <body>
    <outline _name="#toplevel" ancestors="." autogenerated="." comments="." const="." constIf="." default="." description="." enum="." examples="." foreignProperty="." format="." matchType="." minItems="0" namespace="." ontology="." ontologyTermPair="." pattern="." ref="." requireAnyOf="." requireIf="." required="." title="FAIRtracks" type="object" unique="." />
    <outline _name="#version_url" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v1/1.0.1/json/schema/fairtracks.schema.json" />
    <outline _name="@schema" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v1/current/json/schema/fairtracks.schema.json" description="The absolute URL of the most recent major version of the relevant FAIRtracks JSON Schema that is backwards-compatible with the current JSON document. Must match the value of '$id' in the linked schema" format="uri" pattern="^(https?|ftp)://" required="true" type="string" />
    <outline _name="doc_info" description="Version and related information about the current FAIRtracks JSON document" required="true" type="object">
      <outline _name="local_id" autogenerated="true" description="Identifier for this FAIRtracks JSON document (within the TrackFind database)" examples="0" required="false" type="string" unique="true" />
      <outline _name="doc_url" autogenerated="true" description="URL to this FAIRtracks JSON document" examples="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v1/1.0.1/json/examples/fairtracks.example.json" format="uri" pattern="^(https?|ftp)://" required="false" type="string" />
      <outline _name="doc_ontology_versions" description="URLs to the version of the ontologies used in the JSON document" required="true" type="object">
        <outline _name="http://edamontology.org/EDAM.owl" autogenerated="true" examples="http://edamontology.org/EDAM_1.21.owl" format="uri" pattern="^http://edamontology.org/EDAM_[0-9]+\.[0-9]+.owl$" required="true" type="string" />
        <outline _name="http://purl.obolibrary.org/obo/cl.owl" autogenerated="true" examples="http://purl.obolibrary.org/obo/cl/releases/2019-08-12/cl.owl" format="uri" pattern="^http://purl.obolibrary.org/obo/cl/releases/[0-9]+-[0-9]+-[0-9]+/cl.owl$" required="true" type="string" />
        <outline _name="http://www.ebi.ac.uk/efo/efo.owl" autogenerated="true" examples="https://github.com/EBISPOT/efo/releases/download/v3.12.0/efo.owl" format="uri" pattern="^https://github.com/EBISPOT/efo/releases/download/v[0-9]+.[0-9]+.[0-9]+/efo.owl$" required="true" type="string" />
        <outline _name="http://purl.obolibrary.org/obo/ncit.owl" autogenerated="true" examples="http://purl.obolibrary.org/obo/ncit/releases/2019-09-09/ncit.owl" format="uri" pattern="^http://purl.obolibrary.org/obo/ncit/releases/[0-9]+-[0-9]+-[0-9]+/ncit.owl$" required="true" type="string" />
        <outline _name="http://purl.obolibrary.org/obo/obi.owl" autogenerated="true" examples="http://purl.obolibrary.org/obo/obi/2019-11-12/obi.owl" format="uri" pattern="^http://purl.obolibrary.org/obo/obi/[0-9]+-[0-9]+-[0-9]+/obi.owl$" required="true" type="string" />
        <outline _name="http://purl.obolibrary.org/obo/so.owl" autogenerated="true" examples="https://raw.githubusercontent.com/The-Sequence-Ontology/SO-Ontologies/v3.1/releases/so-xp.owl/so.owl" format="uri" pattern="^https://raw.githubusercontent.com/The-Sequence-Ontology/SO-Ontologies/v[0-9]+\.[0-9]+/releases/so-xp.owl/so.owl$" required="true" type="string" />
        <outline _name="http://purl.obolibrary.org/obo/uberon.owl" autogenerated="true" examples="http://purl.obolibrary.org/obo/uberon/releases/2019-06-27/uberon.owl" format="uri" pattern="^http://purl.obolibrary.org/obo/uberon/releases/[0-9]+-[0-9]+-[0-9]+/uberon.owl$" required="true" type="string" />
      </outline>
      <outline _name="doc_version" description="Version of this FAIRtracks JSON document" required="true" type="string" />
      <outline _name="doc_date" description="Creation date of this version of this FAIRtracks document" format="date-time" required="true" type="string" />
    </outline>
    <outline _name="collection_info" description="General information about the track collection that is annotated in the current FAIRtracks JSON document" required="true" type="object">
      <outline _name="source_repo_url" description="URL to the track repository containing the collection (e.g., the Track Hub Registry)" examples="https://www.encodeproject.org/search" format="uri" pattern="^(https?|ftp)://" required="true" type="string" />
      <outline _name="local_id" autogenerated="true" description="Submitter-local identifier (within track repository) for the collection" examples="encode_example" required="false" type="string" unique="true" />
      <outline _name="doc_ref" autogenerated="true" description="Reference to the JSON document containing the study (using the  identifier of the document)" examples="0" foreignProperty="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v1/1.0.1/json/schema/fairtracks.schema.json#doc_info/local_id" format="foreign_ref" required="false" type="string" />
      <outline _name="short_name" description="Short name of the track collection. Suggested maximum length is 17 characters" examples="ENCODE example" required="true" type="string" />
      <outline _name="long_name" description="Long name of the track collection. Suggested maximum length is 80 characters" examples="Example of a few tracks created by from the ENCODE Project" required="true" type="string" />
      <outline _name="description_url" description="URL to a web page or file describing the track collection" examples="https://www.encodeproject.org/help/project-overview/" format="uri" pattern="^(https?|ftp)://" required="false" type="string" />
      <outline _name="orig_metadata_url" description="URL to track collection metadata in its original form (might contain more than the relevant metadata)" examples="https://www.encodeproject.org/search/?format=json" format="uri" pattern="^(https?|ftp)://" required="false" type="string" />
      <outline _name="contact" description="Contact information for the track collection" ref="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v1/1.0.1/json/schema/fairtracks_contact.schema.json" required="true" type="object" />
    </outline>
    <outline _name="studies" description="Array of JSON documents describing different studies that have produced data for the track collection" minItems="1" required="true" type="array">
      <outline ref="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v1/1.0.1/json/schema/fairtracks_study.schema.json" />
    </outline>
    <outline _name="experiments" description="Array of JSON documents describing the different experiments that have produced data for the track collection" minItems="1" required="true" type="array">
      <outline ref="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v1/1.0.1/json/schema/fairtracks_experiment.schema.json" />
    </outline>
    <outline _name="samples" description="Array of JSON documents describing the different samples that have been analyzed to create the track collection" minItems="1" required="true" type="array">
      <outline ref="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v1/1.0.1/json/schema/fairtracks_sample.schema.json" />
    </outline>
    <outline _name="tracks" description="Array of JSON documents describing the different track files in the collection" minItems="1" required="true" type="array">
      <outline ref="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/v1/1.0.1/json/schema/fairtracks_track.schema.json" />
    </outline>
  </body>
</opml>
