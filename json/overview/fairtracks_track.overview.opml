<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <body>
    <outline _text="#title" anyOf="." autogenerated="." comments="." const="FAIRification of Genomic Tracks JSON Schema - Track" default="." description="." enum="." examples=".|.|.|." foreignProperty="." format="." matchType="." minItems="0" namespace="." ontology="." pattern="." required="." type="string" unique="." />
    <outline _text="@schema" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_track.schema.json" description="The JSON schema absolute URI. Used for link JSON data to a particular version of the JSON schema. Must match the value of '$id' in the linked schema" format="iri" required="false" type="string" />
    <outline _text="global_id" autogenerated="true" description="Global track identifier, resolvable by identifiers.org [to be created by us]" examples="fairtracks:1|fairtracks:2|fairtracks:3|fairtracks:4" format="curie" matchType="basic" namespace="fairtracks" required="false" primary_key="true" type="string" />
    <outline _text="local_id" description="Submitter-local identifier (within investigation/hub) for study (in curie-format, if applicable)" examples="encode:ENCFF625ZYB|encode:ENCFF718EPO|encode:ENCFF717PIO|encode:ENCFF955LOC" required="true" type="string" unique="true" />
    <outline _text="experiment_ref" description="Reference to the experiment of the track (using the submitter-local identifier of the sample)" examples="encode:ENCSR000DQP|encode:ENCSR000DQP|encode:ENCSR000DQP|encode:ENCSR000DQP" required="true" type="string" />
    <outline _text="file_iri" description="A IRI to the track data file" examples="https://www.encodeproject.org/files/ENCFF625ZYB/@@download/ENCFF625ZYB.bed.gz|https://www.encodeproject.org/files/ENCFF718EPO/@@download/ENCFF718EPO.bigBed|https://www.encodeproject.org/files/ENCFF717PIO/@@download/ENCFF717PIO.bigWig|https://www.encodeproject.org/files/ENCFF955LOC/@@download/ENCFF955LOC.bigWig" format="iri" required="true" type="string" />
    <outline _text="file_name" autogenerated="true" description="Name of the track file" examples="r1.narrowPeak.gz|r1.narrowPeak.bb|r1.fc_signal.bw|r1.pvalue_signal.bw" required="false" type="string" />
    <outline _text="label_short" comments=" (min of characters limits [14?])" description="A short label of the track file [less than 14 characters?]" examples="ENCFF625ZYB|ENCFF718EPO|ENCFF717PIO| ENCFF955LOC" required="true" type="string" />
    <outline _text="label_long" description="A long label of the track file" examples="H3K4me3 ChIP-seq of B cell peaks rep1 ENCSR000DQP - ENCFF625ZYB|H3K4me3 ChIP-seq of B cell peaks rep1 ENCSR000DQP - ENCFF718EPO|H3K4me3 ChIP-seq of B cell fold change over control rep1 ENCSR000DQP - ENCFF717PIO|H3K4me3 ChIP-seq of B cell signal p-value rep1 ENCSR000DQP - ENCFF955LOC" required="true" type="string" />
    <outline _text="genome_assembly" description="An assembly identifier (using UCSC nomenclature)" examples="GRCh38|GRCh38|GRCh38|GRCh38" required="true" type="string" />
    <outline _text="file_format" description="File format of the track data file" required="true" type="object">
      <outline _text="term_iri" description="File format (IRI linking to an EDAM ontology term)" examples="http://edamontology.org/format_3613|http://edamontology.org/format_3004|http://edamontology.org/format_3006|http://edamontology.org/format_3006" format="term" matchType="exact" ontology="http://edamontology.org/EDAM_1.21.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" description="File format (exact value according to the EDAM ontology)" examples="ENCODE narrow peak format|bigBed|bigWig|bigWig" required="false" type="string" />
    </outline>
    <outline _text="content_type" description="Type of content represented by the track data file (narrow peaks, signal, region, etc.)" required="true" type="object">
      <outline _text="term_iri" comments="(data subtree in EDAM, under &quot;Annotation track&quot;? Contact EDAM to improve the ontology in relation to this)" description="Content type (IRI linking to an EDAM ontology term)" examples="http://edamontology.org/data_3002|http://edamontology.org/data_3002|http://edamontology.org/data_3002|http://edamontology.org/data_3002" format="term" matchType="exact" ontology="http://edamontology.org/EDAM_1.21.owl" required="true " type="string" />
      <outline _text="term_value" autogenerated="true" description="Content type (exact value according to the EDAM ontology)" examples="Annotation track|Annotation track|Annotation track|Annotation track" required="false" type="string" />
    </outline>
    <outline _text="checksum" required="true" type="object">
      <outline _text="cs_method" description="Method of checksum generation" enum="MD5|SHA-256" examples="MD5|MD5|MD5|MD5" required="true" type="string" />
      <outline _text="cs_hash" description="Checksum of track file, using the method described in cs_method" examples="0ab98dbd2a2193ab3e09df030ea4f934|6d2c6c99a7407f1c49ab163e41d9b575|550ae974bf94f1deb1676613ab24a5da|942ea96e86313d4338cf3fd020882c4d" required="true" type="string" />
    </outline>
  </body>
</opml>