<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <body>
    <outline _name="#toplevel" ancestors="." autogenerated="." comments="." const="." constIf="." default="." description="." enum="." examples=".|.|.|." foreignProperty="." format="." matchType="." minItems="0" namespace="." ontology="." pattern="." ref="." requireAnyOf="." requireIf="." required="." title="Track" type="object" unique="." />
    <outline _name="@schema" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_track.schema.json" description="The JSON Schema absolute URL. Used to link JSON data to a JSON schema. Must match the value of '$id' in the linked schema" format="uri" pattern="^(https?|ftp)://" required="false" type="string" />
    <outline _name="global_id" autogenerated="true" comments="curie and matchType to be added later" description="Global track identifier, resolvable by identifiers.org [to be created by us]" examples="fairtracks:1|fairtracks:2|fairtracks:3|fairtracks:4" namespace="fairtracks" required="false" type="string" />
    <outline _name="local_id" description="Submitter-local identifier (within investigation/hub) for study (in CURIE-format, if applicable)" examples="encode:ENCFF625ZYB|encode:ENCFF718EPO|encode:ENCFF717PIO|encode:ENCFF955LOC" required="true" type="string" unique="true" />
    <outline _name="experiment_ref" description="Reference to the experiment of the track (using the submitter-local identifier of the sample)" examples="encode:ENCSR000DQP|encode:ENCSR000DQP|encode:ENCSR000DQP|encode:ENCSR000DQP" foreignProperty="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_experiment.schema.json#local_id" format="foreign_ref" required="true" type="string" />
    <outline _name="raw_file_ids" minItems="1" required="false" type="array">
      <outline description="List of identifiers to raw data files used to create track (typically BAM), resolvable by identifiers.org" examples="encode: ENCFF207FGO;encode:ENCFF842FEQ|encode:ENCFF207FGO;encode:ENCFF842FEQ|encode:ENCFF207FGO;encode:ENCFF842FEQ|encode:ENCFF207FGO;encode:ENCFF842FEQ" format="curie" matchType="canonical" required="true" type="string" />
    </outline>
    <outline _name="file_url" description="A URL to the track data file" examples="https://www.encodeproject.org/files/ENCFF625ZYB/@@download/ENCFF625ZYB.bed.gz|https://www.encodeproject.org/files/ENCFF718EPO/@@download/ENCFF718EPO.bigBed|https://www.encodeproject.org/files/ENCFF717PIO/@@download/ENCFF717PIO.bigWig|https://www.encodeproject.org/files/ENCFF955LOC/@@download/ENCFF955LOC.bigWig" format="uri" pattern="^(https?|ftp|rsync)://" required="true" type="string" />
    <outline _name="file_name" autogenerated="true" description="Name of the track file" examples="r1.narrowPeak.gz|r1.narrowPeak.bb|r1.fc_signal.bw|r1.pvalue_signal.bw" required="false" type="string" />
    <outline _name="label_short" description="A short label of the track file. Suggested maximum length is 17 characters" examples="ENCFF625ZYB|ENCFF718EPO|ENCFF717PIO| ENCFF955LOC" required="true" type="string" />
    <outline _name="label_long" description="A long label of the track file. Suggested maximum length is 80 characters" examples="H3K4me3 ChIP-seq of B cell peaks rep1 ENCSR000DQP - ENCFF625ZYB|H3K4me3 ChIP-seq of B cell peaks rep1 ENCSR000DQP - ENCFF718EPO|H3K4me3 ChIP-seq of B cell fold change over control rep1 ENCSR000DQP - ENCFF717PIO|H3K4me3 ChIP-seq of B cell signal p-value rep1 ENCSR000DQP - ENCFF955LOC" required="true" type="string" />
    <outline _name="genome_assembly" description="An assembly identifier (using UCSC nomenclature)" examples="GRCh38|GRCh38|GRCh38|GRCh38" required="true" type="string" />
    <outline _name="file_format" description="File format of the track data file" required="true" type="object">
      <outline _name="term_id" description="URL linking to an ontology term" examples="http://edamontology.org/format_3613|http://edamontology.org/format_3004|http://edamontology.org/format_3006|http://edamontology.org/format_3006" format="term" matchType="exact" ontology="http://edamontology.org/EDAM.owl" pattern="^(https?|ftp)://" required="true" type="string" />
      <outline _name="term_label" autogenerated="true" description="Exact value according to the ontology used" examples="ENCODE narrow peak format|bigBed|bigWig|bigWig" required="false" type="string" />
    </outline>
    <outline _name="content_type" description="Type of content represented by the track data file (narrow peaks, signal, region, etc.)" required="true" type="object">
      <outline _name="term_id" comments="(data subtree in EDAM, under &quot;Annotation track&quot;? Contact EDAM to improve the ontology in relation to this)" description="URL linking to an ontology term" examples="http://edamontology.org/data_3002|http://edamontology.org/data_3002|http://edamontology.org/data_3002|http://edamontology.org/data_3002" format="term" matchType="exact" ontology="http://edamontology.org/EDAM.owl" pattern="^(https?|ftp)://" required="true " type="string" />
      <outline _name="term_label" autogenerated="true" description="Exact value according to the ontology used" examples="Annotation track|Annotation track|Annotation track|Annotation track" required="false" type="string" />
    </outline>
    <outline _name="checksum" required="true" type="object">
      <outline _name="cs_method" description="Method of checksum generation" enum="MD5|SHA-256" examples="MD5|MD5|MD5|MD5" required="true" type="string" />
      <outline _name="cs_hash" description="Checksum of track file, using the method described in cs_method" examples="0ab98dbd2a2193ab3e09df030ea4f934|6d2c6c99a7407f1c49ab163e41d9b575|550ae974bf94f1deb1676613ab24a5da|942ea96e86313d4338cf3fd020882c4d" required="true" type="string" />
    </outline>
  </body>
</opml>
