  </head>
  <body>
    <outline text="@schema" description="The JSON schema absolute URI. Used for link JSON data to a particular version of the JSON schema. Must match the value of &apos;$id&apos; in the linked schema" required="false" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_track.schema.json" type="string" format="iri"/>
    <outline text="global_id" description="Global track identifier, resolvable by identifiers.org [to be created by us]" required="true" autogenerated="true" type="string" format="curie" namespace="fairtracks" matchType="canonical" examples="fairtracks:1|fairtracks:2|fairtracks:3|fairtracks:4"/>
    <outline text="local_id" description="Submitter-local identifier (within investigation/hub) for study (in curie-format, if applicable)" required="true" unique="true" type="string" examples="encode:ENCFF625ZYB|encode:ENCFF718EPO|encode:ENCFF717PIO|encode:ENCFF955LOC"/>
    <outline text="experiment_ref" description="Reference to the experiment of the track (using the submitter-local identifier of the sample)" required="true" type="string" examples="encode:ENCSR000DQP|encode:ENCSR000DQP|encode:ENCSR000DQP|encode:ENCSR000DQP"/>
    <outline text="file_iri" description="A IRI to the track data file" required="true" type="string" format="iri" examples="https://www.encodeproject.org/files/ENCFF625ZYB/@@download/ENCFF625ZYB.bed.gz|https://www.encodeproject.org/files/ENCFF718EPO/@@download/ENCFF718EPO.bigBed|https://www.encodeproject.org/files/ENCFF717PIO/@@download/ENCFF717PIO.bigWig|https://www.encodeproject.org/files/ENCFF955LOC/@@download/ENCFF955LOC.bigWig"/>
    <outline text="file_name" description="Name of the track file" required="false" autogenerated="true" type="string" examples="r1.narrowPeak.gz|r1.narrowPeak.bb|r1.fc_signal.bw|r1.pvalue_signal.bw"/>
    <outline text="label_short" description="A short label of the track file [less than 14 characters?]" required="true" type="string" examples="ENCFF625ZYB|ENCFF718EPO|ENCFF717PIO| ENCFF955LOC" comments=" (min of characters limits [14?])"/>
    <outline text="label_long" description="A long label of the track file" required="true" type="string" examples="H3K4me3 ChIP-seq of B cell peaks rep1 ENCSR000DQP - ENCFF625ZYB|H3K4me3 ChIP-seq of B cell peaks rep1 ENCSR000DQP - ENCFF718EPO|H3K4me3 ChIP-seq of B cell fold change over control rep1 ENCSR000DQP - ENCFF717PIO|H3K4me3 ChIP-seq of B cell signal p-value rep1 ENCSR000DQP - ENCFF955LOC"/>
    <outline text="genome_assembly" description="An assembly identifier (using UCSC nomenclature)" required="true" type="string" examples="GRCh38|GRCh38|GRCh38|GRCh38"/>
    <outline text="file_format" description="File format of the track data file" required="true" type="object">
      <outline text="term_iri" description="File format (IRI linking to an ontology term)" required="true" type="string" format="term" ontology="http://edamontology.org/EDAM_1.21.owl" matchType="exact" examples="http://edamontology.org/format_3613|http://edamontology.org/format_3004|http://edamontology.org/format_3006|http://edamontology.org/format_3006"/>
      <outline text="term_value" description="File format (exact value according to the ontology used)" required="false" autogenerated="true" type="string" examples="ENCODE narrow peak format|bigBed|bigWig|bigWig"/>
    </outline>
    <outline text="content_type" description="Type of content represented by the track data file (e.g. narrow peaks, signal, region, â€¦)" required="true" type="object">
      <outline text="datatype_url" description="Content type (IRI linking to an ontology term)" required="true " type="string" format="term" ontology="http://edamontology.org/EDAM_1.21.owl" matchType="exact" examples="http://edamontology.org/data_3002|http://edamontology.org/data_3002|http://edamontology.org/data_3002|http://edamontology.org/data_3002" comments="(data subtree in EDAM, under &quot;Annotation track&quot;? Contact EDAM to improve the ontology in relation to this)"/>
      <outline text="datatype_term" description="Content type (exact value according to the ontology used)" required="false" autogenerated="true" type="string" examples="Annotation track|Annotation track|Annotation track|Annotation track"/>
    </outline>
    <outline text="checksum" required="true" type="object">
      <outline text="cs_method" description="Method of checksum generation" required="true" enum="MD5|SHA-256" type="string" examples="MD5|MD5|MD5|MD5"/>
      <outline text="cs_hash" description="Checksum of track file, using the method described in cs_method" required="true" type="string" examples="0ab98dbd2a2193ab3e09df030ea4f934|6d2c6c99a7407f1c49ab163e41d9b575|550ae974bf94f1deb1676613ab24a5da|942ea96e86313d4338cf3fd020882c4d"/>
    </outline>
  </body>
