  <body>
    <outline text="fair_tracks" type="object" required="true" autogenerated="false">
      <outline text="_schema" description="The JSON schema absolute URI. It must match the value of &apos;id&apos; in the JSON schema" type="string" format="uri" const="https://www.elixir-europe.org/IS/FAIRGDataTracks/json-schemas/0.1" required="true"/>
      <outline text="experiment" type="object" required="true" autogenerated="false">
        <outline text="id" type="object" required="true">
          <outline text="global_exp_id" description="Global experiment id (possibly generated by TrackFind, and a carrot for making the tracks standardized)" type="string" required="false" examples="123:456:789" autogenerated="true"/>
          <outline text="exp_metadata_version" description="Version of metadata (trackhub) [add date]. Autogenerated by TrackFind" type="string" pattern="^[1-9][0-9]*:[1-9][0-9]*:[1-9][0-9]*$" required="false" examples="123:456:789" autogenerated="true"/>
          <outline text="project_exp_id" description="Submitter-local identifier (within Trackhub/repo) for experiment" type="string" required="true" examples="123:456:789" autogenerated="false"/>
        </outline>
        <outline text="type" type="object" required="true">
          <outline text="exp_type_uri" description="EXPERIMENT_ONTOLOGY_URI" type="term" ontology="http://purl.obolibrary.org/obo/obi/2018-08-27/obi.owl" required="true" examples="http://purl.obolibrary.org/obo/OBI_0000716" autogenerated="false" unused="^(https?)://purk.obolibrary.org/obo/OBI_[0-9]+$" Values="OBI"/>
          <outline text="exp_type_term" description="EXPERIMENT_TYPE. Autogenerated from untology_uri" type="string" required="false" examples="ChIP-seq assay" autogenerated="true" Values="Enum (&quot;DNAme&quot;, &quot;RNA-Seq&quot;...)"/>
        </outline>
        <outline text="target" description="target (main target of dataset, e.g. antibody/disease/gene/... To be used across technologies)" type="string" required="true" examples="MITF" autogenerated="false" Comments=" (free text, alongside experiment type. Description on what kind of content should go there, but up to each project)"/>
        <outline text="contact_orcid_id" description="Contact e-mail address" type="curie" namespace="orcid" required="true" examples="https://orcid.org/0000-0002-9103-1077&#10;" autogenerated="false" unused="^(https?)://orchid.org/[0-9][0-9][0-9][0-9]-[0-9][0-9][0-9][0-9]-[0-9][0-9][0-9][0-9]-[0-9][0-9][0-9][0-9]$" Comments=" (ORCID id - Needs GDPR consent)"/>
        <outline text="lab_protocol_description" description="Field(s) for lab protocol metadata [should include date]" type="string" required="false" examples="Some text including date" autogenerated="false"/>
        <outline text="compute_protocol_description" description="Field(s) for computational protocol metadata [should include date]" type="string" required="false" examples="Some text including date" autogenerated="false" Comments=" (We suggest to create a registry of workflows and parameter settings that one can point to - ELIXIR follow-up project?)"/>
      </outline>
      <outline text="sample" type="object" required="true" autogenerated="false">
        <outline text="id" type="object" required="true">
          <outline text="biosample_id" description="BioSampleID" type="string" required="true" examples="SAMN08738466" autogenerated="false" Values="BioSampleID"/>
          <outline text="project_sample_id" description="Submitter-local identifier (within Trackhub/repo) for sample" type="string" required="true" examples="123:456:789" autogenerated="false"/>
        </outline>
        <outline text="type" type="object" required="true">
          <outline text="sample_type_uri" description="Uri to ontology term describing sample type (EFO, CL or UBERON depending on type)" type="term" ontology="http://www.ebi.ac.uk/efo/releases/2018-02-15/efo.owl, http://purl.obolibrary.org/obo/cl/releases/2018-07-07/cl.owl, http://purl.obolibrary.org/obo/uberon/releases/2018-07-30/uberon.owl" required="true" examples="http://www.ebi.ac.uk/efo/EFO_0003042" autogenerated="false" Values="EFO, CL or UBERON depending on type"/>
          <outline text="sample_type_term" description="Sample type (human readable). Autogenerated from sample_type_uri" type="string" required="false" examples="H1-hESC" autogenerated="true"/>
          <outline text="biomaterial_type" description="BIOMATERIAL_TYPE" type="string" required="true " examples="Cell line" autogenerated="false" Values="Enum (&quot;Cell Line&quot;, &quot;Primary tissue&quot;...)" Comments=" (might also be supported in EDAM)"/>
        </outline>
      </outline>
      <outline text="study" type="object" required="true" autogenerated="false">
        <outline text="id" type="object" required="true">
          <outline text="project_name" type="string" required="true" examples="MyProject" autogenerated="false" Values="&quot;FAANG&quot;"/>
          <outline text="project_study_id" description="Submitter-local identifier (within Trackhub/repo) for all object types (study, sample, experiment.. [col2])" type="string" required="true" examples="123:456:789" autogenerated="false"/>
          <outline text="pubmed_id" description="Pubmed id (dataset / publication)" type="curie" namespace="pubmed" required="false" examples="28459977" autogenerated="false"/>
        </outline>
        <outline text="phenotype" type="object" required="true">
          <outline text="disease_uri" description="DISEASE_ONTOLOGY_URI" type="term" namespace="http://purl.obolibrary.org/obo/ncit/releases/2018-04-05/ncit.owl" required="true" examples="https://ncim.nci.nih.gov/ncimbrowser/ConceptReport.jsp?dictionary=NCI%20Metathesaurus&amp;code=C1272460" autogenerated="false" Values="NCImetathesaurus"/>
          <outline text="disease_term" description="DISEASE. Autogenerated from disease_uri" type="string" required="false" examples="Not Applicable" autogenerated="true" Values="String"/>
        </outline>
      </outline>
      <outline text="tracks" type="array" required="true" autogenerated="false">
        <outline text="track" type="object" required="false">
          <outline text="id" type="object" required="true">
            <outline text="global_track_id" description="Global track id (generated by TrackFind?)" type="string" required="false" examples="123:456:789" autogenerated="true"/>
            <outline text="project_track_id" description="Submitter-local identifier (within Trackhub/repo) for all object types (study, sample, experiment.. [col2])" type="string" required="true" examples="123:456:789" autogenerated="false"/>
          </outline>
          <outline text="url" description="A URL (to track file)" type="string" format="uri" required="true" examples="ftp://myproject.org/files/h1hesc-mitf.bw" autogenerated="false"/>
          <outline text="name" type="object" required="true">
            <outline text="filename" type="string" required="false" examples="h1hesc-mitf.bb" autogenerated="true" Values="string"/>
            <outline text="short_label" description="A short label (string)" type="string" required="true" examples="MITF ChIP-seq (H1-hESC)" autogenerated="false" Comments=" (min of characters limits [14?])"/>
            <outline text="long_label" description="A long label (string)" type="string" required="true" examples="MITF ChIP-seq assay in human embryonic stem cell lineH1-hESC" autogenerated="false"/>
          </outline>
          <outline text="genome_assembly" description="An assembly identifier (using UCSC nomenclature)" type="string" required="true" examples="hg38" autogenerated="false"/>
          <outline text="type" type="object" required="true">
            <outline text="filetype_url" description="filetype" type="string" format="uri" required="true" examples="http://edamontology.org/format_3004" autogenerated="false" Values="EDAM"/>
            <outline text="filetype_term" type="string" required="false" examples="bigBed" autogenerated="true"/>
            <outline text="datatype_uri" description="data type/represented concept (e.g. narrow peaks, signal, single mutations...)" type="string" format="uri" required="true " examples="http://edamontology.org/data_1255" autogenerated="false" Comments="(data subtree in EDAM? Contact EDAM to improve the ontology in relation to this)"/>
            <outline text="datatype_term" type="string" required="false" examples="Sequence features" autogenerated="true"/>
          </outline>
          <outline text="checksum" type="object" required="true">
            <outline text="cs_method" type="string" required="true" examples="MD5" autogenerated="false" Values="Enum" Comments=" (enum)"/>
            <outline text="cs_hash" type="string" required="true" examples="79054025255fb1a26e4bc422aef54eb4" autogenerated="false" Values="string"/>
          </outline>
        </outline>
      </outline>
    </outline>
  </body>
