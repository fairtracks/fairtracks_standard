  <body>
    <outline text="@schema" description="The JSON schema absolute IRI. Used for link JSON data to a particular version of the JSON schema. Must match the value of &apos;id&apos; in the linked schema" required="true" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_sample.schema.json" type="string" format="iri"/>
    <outline text="global_id" description="Global sample identifier, resolvable by identifiers.org" required="true" type="string" namespace="biosample" format="curie" matchType="canonical" examples="biosample:SAMN01731491"/>
    <outline text="local_id" description="Submitter-local identifier (within investigation/hub) for sample (in curie-format, if applicable)" required="true" unique="true" type="string" examples="GEO:GSM945229"/>
    <outline text="biomaterial_type" description="Type of biomaterial" required="true" enum="[&quot;cell line&quot;, &quot;primary cell&quot;, &quot;primary cell culture&quot;, &quot;primary tissue&quot;]" type="string" examples="primary cell" comments="Borrowed from IHEC, should be extended to support e.g. tumors"/>
    <outline text="sample_type" description="Main class of sample (e.g. specific cell line, cell/tissue type, â€¦)" required="true" type="object">
      <outline text="term_iri" description="IRI linking to an ontology term" required="true" type="string" format="term" ontology="https://github.com/EBISPOT/efo/releases/download/v3.5.1/efo.owlFI" matchType="exact" examples="http://purl.obolibrary.org/obo/CL_0000236"/>
      <outline text="term_value" description="Exact value according to the ontology used" required="false" autogenerated="true" type="string" examples="H1-hESC"/>
    </outline>
