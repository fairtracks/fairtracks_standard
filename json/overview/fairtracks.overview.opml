<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <body>
    <outline _text="#title" ancestor="." anyOf="." autogenerated="." comments="." const="FAIRification of Genomic Tracks JSON Schema" constIf="." default="." description="." enum="." examples="." foreignProperty="." format="." matchType="." minItems="0" namespace="." ontology="." pattern="." ref="." requireIf="." required="." type="string" unique="." />
    <outline _text="@schema" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks.schema.json" description="The JSON schema absolute URL. Used for link JSON data to a particular version of the JSON schema. Must match the value of '$id' in the linked schema" format="uri" pattern="^(https?|ftp)://" required="true" type="string" />
    <outline _text="doc_info" required="true" type="object">
      <outline _text="doc_url" description="URL to this FAIRtracks JSON document" examples="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/examples/fairtracks.example.json" format="uri" pattern="^(https?|ftp)://" required="false" type="string" />
      <outline _text="doc_version" description="Version of this FAIRtracks JSON document" required="true" type="string" />
      <outline _text="doc_date" description="Creation date of this version of this FAIRtracks document" format="date-time" required="true" type="string" />
    </outline>
    <outline _text="collection_info" required="true" type="object">
      <outline _text="short_name" description="Short name of the track collection. Suggested maximum length is 17 characters" examples="ENCODE example" required="true" type="string" />
      <outline _text="long_name" description="Long name of the track collection. Suggested maximum length is 80 characters" examples="Example of a few tracks created by from the ENCODE Project" required="true" type="string" />
      <outline _text="description_url" description="URL to a web page or file describing the track collection" examples="https://www.encodeproject.org/help/project-overview/" format="uri" pattern="^(https?|ftp)://" required="false" type="string" />
      <outline _text="source_repo" required="false" type="object">
        <outline _text="repo_url" description="URL to the track repository containing the collection (e.g., the Track Hub Registry)" examples="https://www.encodeproject.org/search" format="uri" pattern="^(https?|ftp)://" required="true" type="string" />
        <outline _text="local_id" description="Submitter-local identifier (within track repository) for the collection" required="false" type="string" />
      </outline>
      <outline _text="orig_metadata_url" description="URL to track collection metadata in its original form (might contain more than the relevant metadata)" examples="https://www.encodeproject.org/search/?format=json" format="uri" pattern="^(https?|ftp)://" required="false" type="string" />
      <outline _text="contact" description="Contact information for the track collection" required="true" type="object">
        <outline _text="name" description="Name of contact person/organization" examples="ENCODE DCC" required="true" type="string" />
        <outline _text="e-mail" anyOf="true" description="E-mail to contact person/organization" examples="encode-help@lists.stanford.edu" format="idn-email" required="false" type="string" />
        <outline _text="orcid" anyOf="true" comments="ORCID - Needs GDPR consent" description="ORCID to contact person" format="curie" matchType="canonical" namespace="orcid" required="false" type="string" />
      </outline>
    </outline>
    <outline _text="studies" minItems="1" required="true" type="array">
      <outline ref="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_study.schema.json" />
    </outline>
    <outline _text="experiments" minItems="1" required="true" type="array">
      <outline ref="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_experiment.schema.json" />
    </outline>
    <outline _text="samples" minItems="1" required="true" type="array">
      <outline ref="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_sample.schema.json" />
    </outline>
    <outline _text="tracks" minItems="1" required="true" type="array">
      <outline ref="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_track.schema.json" />
    </outline>
  </body>
</opml>