<opml version="2.0">
  <body>
    <outline _text="#title" ancestor="." anyOf="." autogenerated="." comments="." const="FAIRification of Genomic Tracks JSON Schema - Sample" default="." description="." enum="." examples="." foreignProperty="." format="." matchType="." minItems="0" namespace="." ontology="." pattern="." requireIf="." required="." type="string" unique="." />
    <outline _text="@schema" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_sample.schema.json" description="The JSON schema absolute URL. Used for link JSON data to a particular version of the JSON schema. Must match the value of 'id' in the linked schema" format="url" required="false" type="string" />
    <outline _text="global_id" description="Global sample identifier, resolvable by identifiers.org" examples="biosample:SAMN01731491" format="curie" matchType="canonical" namespace="biosample" required="false" type="string" />
    <outline _text="local_id" description="Submitter-local identifier (within investigation/hub) for sample (in CURIE-format, if applicable)" examples="GEO:GSM945229" required="true" type="string" unique="true" />
    <outline _text="biospecimen_class" description="Main type of structural unit to be used for classification of biospecimen contents" required="true" type="object">
      <outline _text="term_url" description="URL linking to an NCIT ontology term" enum="http://purl.obolibrary.org/obo/NCIT_C12508|http://purl.obolibrary.org/obo/NCIT_C12801|http://purl.obolibrary.org/obo/NCIT_C16403|http://purl.obolibrary.org/obo/NCIT_C103199 " examples="http://purl.obolibrary.org/obo/NCIT_C16403" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/ncit/releases/2019-09-09/ncit.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" description="Exact value according to the NCIT ontology" enum="Cell|Abnormal Cell|Cell Line|Organism Part" examples="Cell Line" required="false" type="string" />
    </outline>
    <outline _text="cell_type" description="Cell type of isolated normal cells in sample. This property should only be used if biospecimen_class is set to &quot;Cell&quot;." requireIf="biospecimen_class-&gt;term_url=http://purl.obolibrary.org/obo/NCIT_C12508" required="false" type="object">
      <outline _text="term_url" ancestor="http://www.ebi.ac.uk/efo/EFO_0000324" description="URL linking to an EFO ontology term" format="term" matchType="exact" ontology="https://github.com/EBISPOT/efo/releases/download/v3.5.1/efo.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" description="Exact value according to the EFO ontology" required="false" type="string" />
    </outline>
    <outline _text="abnormal_cell_type" description="Cell type of isolated abnormal cells in sample. This property should only be used if biospecimen_class is set to &quot;Abnormal Cell&quot;." requireIf="biospecimen_class-&gt;term_url=http://purl.obolibrary.org/obo/NCIT_C12801" required="false" type="object">
      <outline _text="term_url" ancestor="http://purl.obolibrary.org/obo/NCIT_C12801" description="URL linking to an NCIT ontology term" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/ncit/releases/2019-09-09/ncit.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" description="Exact value according to the EFO ontology" required="false" type="string" />
    </outline>
    <outline _text="cell_line" description="Cultured cell line used in the sample. This property should only be used if biospecimen_class is set to &quot;Cell Line&quot;." requireIf="biospecimen_class-&gt;term_url=http://purl.obolibrary.org/obo/NCIT_C16403" required="false" type="object">
      <outline _text="term_url" ancestor="http://purl.obolibrary.org/obo/CL_0000010" description="URL linking to an EFO ontology term" examples="http://purl.obolibrary.org/obo/CL_0000236" format="term" matchType="exact" ontology="https://github.com/EBISPOT/efo/releases/download/v3.5.1/efo.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" description="Exact value according to the EFO ontology" examples="H1-hESC" required="false" type="string" />
    </outline>
    <outline _text="organism_part" description="Part of organism (typically tissue or organ) from which the sample was taken, or cell line was derived from. This property  must be used is biospecimen_class is set to &quot;Organism Part&quot;, but can also be used for the other values of biospecimen_class." requireIf="biospecimen_class-&gt;term_url=http://purl.obolibrary.org/obo/NCIT_C103199" required="false" type="object">
      <outline _text="term_url" ancestor="http://www.ebi.ac.uk/efo/EFO_0000635" description="URL linking to an EFO ontology term" examples="http://purl.obolibrary.org/obo/CL_0002322" format="term" matchType="exact" ontology="https://github.com/EBISPOT/efo/releases/download/v3.5.1/efo.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" description="Exact value according to the EFO ontology" examples="embryonic stem cell" required="false" type="string" />
    </outline>
    <outline _text="main_type" autogenerated="true" description="Main classification of sample, copied from cell_type, abnormal_cell_type, cell_line, or organism_part, according to biospecimen_class" examples="H1-hESC" required="false" type="string" />
    <outline _text="phenotype" description="Main phenotype (e.g. disease) connected to the sample" required="true" type="object">
      <outline _text="term_url" default="http://purl.obolibrary.org/obo/PATO_0000461" description="URL linking to an EFO ontology term" format="term" matchType="exact" ontology="https://github.com/EBISPOT/efo/releases/download/v3.5.1/efo.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" default="normal" description="Exact value according to the EFO ontology" required="false" type="string" />
    </outline>
  </body>
</opml>