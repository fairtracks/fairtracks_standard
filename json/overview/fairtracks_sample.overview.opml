<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <body>
    <outline _key="#toplevel" ancestors="." autogenerated="." comments="." const="." constIf="." default="." description="." enum="." examples="." foreignProperty="." format="." matchType="." minItems="0" namespace="." ontology="." pattern="." ref="." requireAnyOf="." requireIf="." required="." title="Sample" type="object" unique="." />
    <outline _key="@schema" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_sample.schema.json" description="The JSON Schema absolute URL. Used to link JSON data to a JSON schema. Must match the value of '$id' in the linked schema" format="uri" pattern="^(https?|ftp)://" required="false" type="string" />
    <outline _key="global_id" description="Global sample identifier, resolvable by identifiers.org" examples="biosample:SAMN01731491" format="curie" matchType="canonical" namespace="biosample" required="false" type="string" />
    <outline _key="local_id" description="Submitter-local identifier (within investigation/hub) for sample (in CURIE-format, if applicable)" examples="geo:GSM945229" required="true" type="string" unique="true" />
    <outline _key="species_id" description="Species identifier, resolvable by identifiers.org" examples="taxonomy:9606" format="curie" matchType="canonical" namespace="taxonomy" required="true" type="string" />
    <outline _key="species_name" autogenerated="true" description="Species name according to the NCBI Taxonomy database (https://www.ncbi.nlm.nih.gov/taxonomy)" examples="Homo sapiens" required="false" type="string" />
    <outline _key="biospecimen_class" description="Main type of structural unit to be used for classification of biospecimen contents" required="true" type="object">
      <outline _key="term_id" description="URL linking to an ontology term" enum="http://purl.obolibrary.org/obo/NCIT_C12508|http://purl.obolibrary.org/obo/NCIT_C12913|http://purl.obolibrary.org/obo/NCIT_C16403|http://purl.obolibrary.org/obo/NCIT_C103199" examples="http://purl.obolibrary.org/obo/NCIT_C16403" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/ncit/releases/2019-09-09/ncit.owl" pattern="^(https?|ftp)://" required="true" type="string" />
      <outline _key="term_label" autogenerated="true" description="Exact value according to the ontology used" enum="Cell|Abnormal Cell|Cell Line|Organism Part" examples="Cell Line" required="false" type="string" />
    </outline>
    <outline _key="cell_type" comments="requireIf: Cell" description="Cell type of isolated normal cells in sample. This property should only be used if biospecimen_class is set to &quot;Cell&quot;." requireIf="biospecimen_class/term_id=http://purl.obolibrary.org/obo/NCIT_C12508" required="false" type="object">
      <outline _key="term_id" ancestors="http://www.ebi.ac.uk/efo/EFO_0000324" description="URL linking to an ontology term" format="term" matchType="exact" ontology="https://github.com/EBISPOT/efo/releases/download/v3.5.1/efo.owl" pattern="^(https?|ftp)://" required="true" type="string" />
      <outline _key="term_label" autogenerated="true" description="Exact value according to the ontology used" required="false" type="string" />
    </outline>
    <outline _key="abnormal_cell_type" comments="requireIf: Abnormal Cell" description="Cell type of isolated abnormal cells in sample. This property should only be used if biospecimen_class is set to &quot;Abnormal Cell&quot;." requireIf="biospecimen_class/term_id=http://purl.obolibrary.org/obo/NCIT_C12913" required="false" type="object">
      <outline _key="term_id" ancestors="http://purl.obolibrary.org/obo/NCIT_C12801" description="URL linking to an ontology term" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/ncit/releases/2019-09-09/ncit.owl" pattern="^(https?|ftp)://" required="true" type="string" />
      <outline _key="term_label" autogenerated="true" description="Exact value according to the ontology used" required="false" type="string" />
    </outline>
    <outline _key="cell_line" comments="requireIf: Cell Line" description="Cultured cell line used in the sample. This property should only be used if biospecimen_class is set to &quot;Cell Line&quot;." requireIf="biospecimen_class/term_id=http://purl.obolibrary.org/obo/NCIT_C16403" required="false" type="object">
      <outline _key="term_id" ancestors="http://purl.obolibrary.org/obo/CL_0000010" description="URL linking to an ontology term" examples="http://purl.obolibrary.org/obo/CL_0000236" format="term" matchType="exact" ontology="https://github.com/EBISPOT/efo/releases/download/v3.5.1/efo.owl" pattern="^(https?|ftp)://" required="true" type="string" />
      <outline _key="term_label" autogenerated="true" description="Exact value according to the ontology used" examples="H1-hESC" required="false" type="string" />
    </outline>
    <outline _key="organism_part" comments="requireIf: Organism Part" description="Part of organism (typically tissue or organ) from which the sample was taken, or cell line was derived from. This property  must be used is biospecimen_class is set to &quot;Organism Part&quot;, but can also be used for the other values of biospecimen_class." requireIf="biospecimen_class/term_id=http://purl.obolibrary.org/obo/NCIT_C103199" required="false" type="object">
      <outline _key="term_id" ancestors="http://www.ebi.ac.uk/efo/EFO_0000635" description="URL linking to an ontology term" examples="http://purl.obolibrary.org/obo/CL_0002322" format="term" matchType="exact" ontology="https://github.com/EBISPOT/efo/releases/download/v3.5.1/efo.owl" pattern="^(https?|ftp)://" required="true" type="string" />
      <outline _key="term_label" autogenerated="true" description="Exact value according to the ontology used" examples="embryonic stem cell" required="false" type="string" />
    </outline>
    <outline _key="phenotype" description="Main phenotype (e.g. disease) connected to the sample" ref="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_phenotype.schema.json" required="true" type="object" />
    <outline _key="main_type" autogenerated="true" description="Main classification of sample, copied from cell_type, abnormal_cell_type, cell_line, or organism_part, according to biospecimen_class" examples="H1-hESC" required="false" type="string" />
  </body>
</opml>
