  <body>
    <outline _text="#title" anyOf="." autogenerated="." comments="." const="FAIRification of Genomic Tracks JSON Schema - Study" description="." enum="." examples="." foreignProperty="." format="." matchType="." minItems="0" namespace="." ontology="." pattern="." required="." type="string" unique="." />
    <outline _text="@schema" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_study.schema.json" description="The JSON schema absolute URI. Used for link JSON data to a particular version of the JSON schema. Must match the value of '$id' in the linked schema" format="iri" required="false" type="string" />
    <outline _text="global_id" description="Global study identifier, resolvable by identifiers.org" examples="GEO:GSE35583" format="curie" matchType="canonical" namespace="geo" required="true" type="string" />
    <outline _text="local_id" description="Submitter-local identifier (within investigation/hub) for study" examples="UW_ChipSeq" required="true" type="string" unique="true" />
    <outline _text="study_name" description="Name of the study" examples="Histone Modifications by ChIP-seq from ENCODE/University of Washington" required="true" type="string" />
    <outline _text="publications" required="false" type="array">
      <outline description="Pubmed identifiers (dataset or publication)" examples="22955617" format="curie" matchType="loose" minItems="1" namespace="pubmed" type="string" />
    </outline>
    <outline _text="contact" description="Contact information for study" required="true" type="object">
      <outline _text="name" description="Name of contact person/organization" examples="ENCODE DCC" required="true" type="string" />
      <outline _text="e-mail" anyOf="true" description="E-mail to contact person/organization" examples="encode-help@lists.stanford.edu" format="idn-email" required="false" type="string" />
      <outline _text="orcid_id" anyOf="true" comments="ORCID id - Needs GDPR consent" description="ORCID id to contact person" examples="0000-0000-0000-0000" format="curie" matchType="loose" namespace="orcid" required="false" type="string" />
    </outline>
    <outline _text="phenotype" description="Main phenotype (e.g. disease) which is studied" required="true" type="object">
      <outline _text="term_iri" description="IRI linking to an EFO ontology term" examples="http://purl.obolibrary.org/obo/PATO_0000461" format="term" matchType="exact" ontology="https://github.com/EBISPOT/efo/releases/download/v3.5.1/efo.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" description="Exact value according to the EFO ontology" examples="Normal" required="false" type="string" />
    </outline>
