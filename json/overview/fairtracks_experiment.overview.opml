<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <body>
    <outline _name="#toplevel" ancestors="." autogenerated="." comments="." const="." constIf="." default="." description="." enum="." examples="." foreignProperty="." format="." matchType="." minItems="0" namespace="." ontology="." pattern="." ref="." requireAnyOf="." requireIf="." required="." title="Experiment" type="object" unique="." />
    <outline _name="@schema" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_experiment.schema.json" description="The JSON Schema absolute URL. Used to link JSON data to a JSON schema. Must match the value of '$id' in the linked schema" format="uri" pattern="^(https?|ftp)://" required="false" type="string" />
    <outline _name="global_id" description="Global experiment identifier, resolvable by identifiers.org" examples="geo:GSE35583" format="curie" matchType="canonical" namespace="geo|ega.experiment" required="false" type="string" />
    <outline _name="local_id" description="Submitter-local identifier (within investigation/hub) for experiment (in CURIE-format, if applicable)" examples="encode:ENCSR000DQP" required="true" type="string" unique="true" />
    <outline _name="study_ref" description="Reference to the study that generated the sample (using the submitter-local identifier of the study)" examples="UW_ChipSeq" foreignProperty="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_study.schema.json#local_id" format="foreign_ref" required="true" type="string" />
    <outline _name="sample_ref" description="Reference to the sample of the experiment (using the submitter-local identifier of the sample)" examples="geo:GSM945229" foreignProperty="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_sample.schema.json#local_id" format="foreign_ref" requireAnyOf="true" required="false" type="string" />
    <outline _name="aggregated_from" description="References to external experiments used as basis for aggregated data generation (using global experiment identifiers, resolvable by identifiers.org)" requireAnyOf="true" required="false" type="array">
      <outline format="curie" matchType="canonical" minItems="1" namespace="geo" required="false" type="string" />
    </outline>
    <outline _name="technique" description="Main technique used in experiment (e.g., laboratory, computational or statistical technique)" required="true" type="object">
      <outline _name="term_id" ancestors="http://purl.obolibrary.org/obo/OBI_0000011|http://edamontology.org/operation_0004" comments="ancestors: planned process|Operation" description="URL linking to an ontology term" examples="http://purl.obolibrary.org/obo/OBI_0002017" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/obi.owl|http://edamontology.org/EDAM.owl" pattern="^(https?|ftp)://" required="true" type="string" />
      <outline _name="term_label" autogenerated="true" description="Exact value according to the ontology used" examples="histone modification identification by ChIP-Seq assay" required="false" type="string" />
    </outline>
    <outline _name="target" description="Main target of the experiment" required="true" type="object">
      <outline _name="sequence_feature" comments="requireIf: histone modification identification by ChIP-Seq assay|bisulfite sequencing assay|DNase I hypersensitive sites sequencing assay, constIf: bisulfite sequencing assay;open_chromatin_region|DNase I hypersensitive sites sequencing assay;DNAseI_hypersensitive_site" constIf="2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0000748;0/term_id=http://purl.obolibrary.org/obo/SO_0001747|2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0001853;0/term_id=http://purl.obolibrary.org/obo/SO_0000685" description="Sequence feature targeted by the experiment" requireIf="2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0002017|2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0000748|2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0001853" required="false" type="object">
        <outline _name="term_id" ancestors="http://purl.obolibrary.org/obo/SO_0000110" comments="ancestors: sequence_feature" description="URL linking to an ontology term" examples="http://purl.obolibrary.org/obo/SO_0001706" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/so.owl" pattern="^(https?|ftp)://" required="true" type="string" />
        <outline _name="term_label" autogenerated="true" description="Exact value according to the ontology used" examples="H3K4_trimethylation" required="false" type="string" />
      </outline>
      <outline _name="gene_id" comments="requireIf: transcription factor binding site identification by ChIP-Seq assay" description="HGNC identifier for gene targeted by the experiment (e.g., specific transcription factor used as ChIP-seq antibody)." matchType="canonical" namespace="hgnc" requireIf="2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0002019" required="false" type="string" />
      <outline _name="gene_product_type" comments="requireIf: RNA-seq assay|microRNA profiling by high throughput sequencing assay|transcription factor binding site identification by ChIP-Seq assay, constIf: RNA-seq assay;Messenger RNA|microRNA profiling by high throughput sequencing assay;MicroRNA|transcription factor binding site identification by ChIP-Seq assay;Transcription Factor" constIf="2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0001271;0/term_id=http://purl.obolibrary.org/obo/NCIT_C813|2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0001922;0/term_id=http://purl.obolibrary.org/obo/NCIT_C25966|2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0002019;0/term_id=http://purl.obolibrary.org/obo/NCIT_C17207" description="Gene product type targeted by the experiment (e.g., miRNA)" requireIf="2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0001271|2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0001922|2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0002019" required="false" type="object">
        <outline _name="term_id" ancestors="http://purl.obolibrary.org/obo/NCIT_C26548" comments="ancestors: Gene Product" description="URL linking to an ontology term" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/ncit.owl" pattern="^(https?|ftp)://" required="true" type="string" />
        <outline _name="term_label" autogenerated="true" description="Exact value according to the ontology used" required="false" type="string" />
      </outline>
      <outline _name="macromolecular_structure" comments="requireIf: Hi-C assay, constIf: Hi-C assay;Chromatin Structure" constIf="2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0002440;0/term_id=http://purl.obolibrary.org/obo/NCIT_C13402" description="Macromolecular structure targeted by the experiment (e.g., chromatin strucure)" requireIf="2/technique/term_id=http://purl.obolibrary.org/obo/OBI_0002440" required="false" type="object">
        <outline _name="term_id" ancestors="http://purl.obolibrary.org/obo/NCIT_C14134" comments="ancestors: Macromolecular Structure" description="URL linking to an ontology term" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/ncit.owl" pattern="^(https?|ftp)://" required="true" type="string" />
        <outline _name="term_label" autogenerated="true" description="Exact value according to the ontology used" required="false" type="string" />
      </outline>
      <outline _name="phenotype" comments="requireIf=GWAS study" description="Main phenotype (e.g. disease) connected to the sample" examples="." ref="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_phenotype.schema.json" requireIf="2/technique/term_id=http://edamontology.org/topic_3517" required="false" type="object" />
      <outline _name="target_details" description="Important details about the target of the experiment (to be included in the 'target' property)" required="false" type="string" />
      <outline _name="summary" autogenerated="true" description="Summary of 'target', copied from 'sequence_feature', 'gene_id', 'gene_product', or 'macromolecular_structure' (and adding 'target_detail'), according to 'technique'" examples="H3K4_trimethylation" required="false" type="string" />
    </outline>
    <outline _name="lab_protocol_description" comments="Should include date" description="Free-text description of lab protocol, or URL to such description" examples="https://www.encodeproject.org/documents/8f459e88-6344-434f-8f9f-6375a9ff1880/@@download/attachment/CD20%2B_Stam_protocol.pdf" required="false" type="string" />
    <outline _name="compute_protocol_description" comments="Should include date" description="Free-text description of computational protocol, or URL to such description" examples="https://www.encodeproject.org/documents/6f6351d4-9310-4a3b-a3c2-70ecac47b28b/@@download/attachment/ChIP-seq_Mapping_Pipeline_Overview.pdf" required="false" type="string" />
  </body>
</opml>
