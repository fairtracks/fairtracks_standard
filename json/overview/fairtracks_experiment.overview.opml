  <body>
    <outline text="#title" const="FAIRification of Genomic Tracks JSON Schema - Experiment" type="string"/>
    <outline text="@schema" description="The JSON schema absolute IRI. Used for link JSON data to a particular version of the JSON schema. Must match the value of &apos;id&apos; in the linked schema" required="false" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_experiment.schema.json" type="string" format="iri"/>
    <outline text="global_id" description="Global experiment identifier, resolvable by identifiers.org" required="true" type="string" format="curie" namespace="?" matchType="canonical" examples="GEO:GSE35583"/>
    <outline text="local_id" description="Submitter-local identifier (within investigation/hub) for experiment (in curie-format, if applicable)" required="true" unique="true" type="string" examples="encode:ENCSR000DQP"/>
    <outline text="study_ref" description="Reference to the study that generated the sample (using the submitter-local identifier of the study)" required="true" type="string" format="foreign_ref" foreignProperty="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_study.schema.json#local_id" examples="UW_ChipSeq"/>
    <outline text="sample_ref" description="Reference to the sample of the experiment (using the submitter-local identifier of the sample)" required="false" anyOf="true" type="string" format="foreign_ref" foreignProperty="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_sample.schema.json#local_id" examples="GEO:GSM945229"/>
    <outline text="aggregated_from" description="References to external experiments used as basis for aggregated data generation (using global experiment identifiers, resolvable by identifiers.org)" required="false" anyOf="true" type="array">
      <outline text="" type="string" format="curie" namespace="?" matchType="canonical" minItems="1"/>
    </outline>
    <outline text="tech_type" required="true" type="object">
      <outline text="term_iri" description="Type of technology (IRI linking to an ontology term)" required="true" type="string" format="term" ontology="http://purl.obolibrary.org/obo/obi/2018-08-27/obi.owl" matchType="exact" examples="http://purl.obolibrary.org/obo/OBI_0000716"/>
      <outline text="term_value" description="Type of experiment (exact value according to the ontology used)" required="false" autogenerated="true" type="string" examples="ChIP-seq assay"/>
    </outline>
    <outline text="target" description="Main target of experiment, e.g. antibody/disease/gene/... [Needs more details]" required="true" type="object">
      <outline text="term_iri" description="IRI linking to an ontology term" required="true" type="string" format="term" ontology="https://raw.githubusercontent.com/The-Sequence-Ontology/SO-Ontologies/v3.1/releases/so-xp.owl/so.owl|?" matchType="exact" examples="http://purl.obolibrary.org/obo/SO_0001706"/>
      <outline text="term_value" description="Exact value according to the ontology used" required="false" autogenerated="true" type="string" examples="H3K4_trimethylation"/>
    </outline>
    <outline text="lab_protocol_description" description="Free-text description of lab protocol, or IRI to such description" required="false" type="string" examples="https://www.encodeproject.org/documents/8f459e88-6344-434f-8f9f-6375a9ff1880/@@download/attachment/CD20%2B_Stam_protocol.pdf" comments="Should include date"/>
    <outline text="compute_protocol_description" description="Free-text description of computational protocol, or IRI to such description" required="false" type="string" examples="https://www.encodeproject.org/documents/6f6351d4-9310-4a3b-a3c2-70ecac47b28b/@@download/attachment/ChIP-seq_Mapping_Pipeline_Overview.pdf" comments="Should include date. (We suggest to create a registry of workflows and parameter settings that one can point to - ELIXIR follow-up project?)"/>
  </body>
