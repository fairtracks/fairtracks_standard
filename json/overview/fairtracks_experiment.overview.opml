<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <body>
    <outline _text="#title" ancestors="." anyOf="." autogenerated="." comments="." const="FAIRification of Genomic Tracks JSON Schema - Experiment" constIf="." default="." description="." enum="." examples="." foreignProperty="." format="." matchType="." minItems="0" namespace="." ontology="." pattern="." ref="." requireIf="." required="." type="string" unique="." />
    <outline _text="@schema" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_experiment.schema.json" description="The JSON schema absolute URL. Used for link JSON data to a particular version of the JSON schema. Must match the value of 'id' in the linked schema" format="uri" pattern="^(https?|ftp)://" required="false" type="string" />
    <outline _text="global_id" description="Global experiment identifier, resolvable by identifiers.org" examples="geo:GSE35583" format="curie" matchType="canonical" namespace="geo|ega.experiment" required="false" type="string" />
    <outline _text="local_id" description="Submitter-local identifier (within investigation/hub) for experiment (in CURIE-format, if applicable)" examples="encode:ENCSR000DQP" required="true" type="string" unique="true" />
    <outline _text="study_ref" description="Reference to the study that generated the sample (using the submitter-local identifier of the study)" examples="UW_ChipSeq" foreignProperty="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_study.schema.json#local_id" format="foreign_ref" required="true" type="string" />
    <outline _text="sample_ref" anyOf="true" description="Reference to the sample of the experiment (using the submitter-local identifier of the sample)" examples="geo:GSM945229" foreignProperty="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_sample.schema.json#local_id" format="foreign_ref" required="false" type="string" />
    <outline _text="aggregated_from" anyOf="true" description="References to external experiments used as basis for aggregated data generation (using global experiment identifiers, resolvable by identifiers.org)" required="false" type="array">
      <outline format="curie" matchType="canonical" minItems="1" namespace="geo" required="false" type="string" />
    </outline>
    <outline _text="technique" description="Main technique used in experiment (e.g., laboratory, computational or statistical technique)" required="true" type="object">
      <outline _text="term_url" ancestors="http://purl.obolibrary.org/obo/OBI_0000011|http://edamontology.org/operation_0004" description="URL linking to an ontology term" examples="http://purl.obolibrary.org/obo/OBI_0000716" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/obi/2018-08-27/obi.owl|http://edamontology.org/EDAM_1.21.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" description="Exact value according to the ontology used" examples="ChIP-seq assay" required="false" type="string" />
    </outline>
    <outline _text="sequence_feature" description="Sequence feature targeted by the experiment" required="false" type="object">
      <outline _text="term_url" ancestors="http://purl.obolibrary.org/obo/SO_0000110" description="URL linking to an ontology term" examples="http://purl.obolibrary.org/obo/SO_0001706" format="term" matchType="exact" ontology="https://raw.githubusercontent.com/The-Sequence-Ontology/SO-Ontologies/v3.1/releases/so-xp.owl/so.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" description="Exact value according to the ontology used" examples="H3K4_trimethylation" required="false" type="string" />
    </outline>
    <outline _text="gene_id" description="HGNC identifier for gene targeted by the experiment (e.g., specific transcription factor used as ChIP-seq antibody)." matchType="canonical" namespace="hgnc" required="false" type="string" />
    <outline _text="gene_product" description="Gene product targeted by the experiment (e.g., miRNA)" required="false" type="object">
      <outline _text="term_url" ancestors="http://purl.obolibrary.org/obo/NCIT_C26548" description="URL linking to an ontology term" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/ncit/releases/2019-09-09/ncit.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" description="Exact value according to the ontology used" required="false" type="string" />
    </outline>
    <outline _text="macromolecular_structure" description="Macromolecular structure targeted by the experiment (e.g., chromatin strucure)" required="false" type="object">
      <outline _text="term_url" ancestors="http://purl.obolibrary.org/obo/NCIT_C14134" description="URL linking to an ontology term" format="term" matchType="exact" ontology="http://purl.obolibrary.org/obo/ncit/releases/2019-09-09/ncit.owl" required="true" type="string" />
      <outline _text="term_value" autogenerated="true" description="Exact value according to the ontology used" required="false" type="string" />
    </outline>
    <outline _text="target_details" description="Important details about the target of the experiment (to be included in the 'target' property)" required="false" type="string" />
    <outline _text="target" autogenerated="true" description="Main target of experiment, copied from 'sequence_feature', 'gene_id', 'gene_product', or 'macromolecular_structure' (and adding 'target_detail'), according to 'technique'" examples="H3K4_trimethylation" required="true" type="string" />
    <outline _text="lab_protocol_description" comments="Should include date" description="Free-text description of lab protocol, or URL to such description" examples="https://www.encodeproject.org/documents/8f459e88-6344-434f-8f9f-6375a9ff1880/@@download/attachment/CD20%2B_Stam_protocol.pdf" required="false" type="string" />
    <outline _text="compute_protocol_description" comments="Should include date. (We suggest to create a registry of workflows and parameter settings that one can point to - ELIXIR follow-up project?)" description="Free-text description of computational protocol, or URL to such description" examples="https://www.encodeproject.org/documents/6f6351d4-9310-4a3b-a3c2-70ecac47b28b/@@download/attachment/ChIP-seq_Mapping_Pipeline_Overview.pdf" required="false" type="string" />
  </body>
</opml>