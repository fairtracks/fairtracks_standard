  <body>
    <outline text="@schema" description="The JSON schema absolute URI. Used for link JSON data to a particular version of the JSON schema. Must match the value of &apos;$id&apos; in the linked schema" required="true" const="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_study.schema.json" type="string" format="iri" examples="https://raw.githubusercontent.com/fairtracks/fairtracks_standard/master/json/schema/fairtracks_study.schema.json"/>
    <outline text="global_id" description="Global study identifier, resolvable by identifiers.org" required="true" type="string" format="curie" namespace="?" matchType="canonical" examples="GEO:GSE35583"/>
    <outline text="local_id" description="Submitter-local identifier (within investigation/hub) for study" required="true" unique="true" type="string" examples="UW_ChipSeq"/>
    <outline text="study_name" description="Name of the study" required="true" type="string" examples="Histone Modifications by ChIP-seq from ENCODE/University of Washington"/>
    <outline text="publications" required="false" type="array">
      <outline text="pubmed_id" description="Pubmed identifiers (dataset or publication)" required="false" type="string" format="curie" namespace="pubmed" matchType="loose" examples="22955617"/>
    </outline>
    <outline text="contact" description="Contact information for study" required="true" type="object">
      <outline text="name" description="Name of contact person/organization" required="true" type="string" examples="ENCODE DCC"/>
      <outline text="e-mail" description="E-mail to contact person/organization" required="true" anyOf="A" type="string" format="idn-email" examples="encode-help@lists.stanford.edu"/>
      <outline text="orcid_id" description="ORCID id to contact person" required="true" anyOf="A" type="string" format="curie" namespace="orcid" matchType="loose" examples="0000-0000-0000-0000" comments="ORCID id - Needs GDPR consent"/>
    </outline>
    <outline text="phenotype" description="Main phenotype (e.g. disease) which is studied" required="true" type="object">
      <outline text="term_iri" description="IRI linking to an ontology term" required="true" type="string" format="term" ontology="?" matchType="exact" examples="http://purl.obolibrary.org/obo/PATO_0000461"/>
      <outline text="term_value" description="Exact value according to the ontology used" required="false" autogenerated="true" type="string" examples="Normal"/>
    </outline>
  </body>
