    <title>basic_example</title>
    <dateCreated>Wed, 12 Dec 2018 10:17:28 GMT</dateCreated>
    <expansionState>0,1,11,17,23</expansionState>
    <vertScrollState>16</vertScrollState>
    <windowTop>212</windowTop>
    <windowLeft>38</windowLeft>
    <windowRight>1820</windowRight>
    <windowBottom>1177</windowBottom>
  </head>
  <body>
    <outline text="fair_tracks" type="object" required="true" autogenerated="false">
      <outline text="experiment" type="object" required="true" autogenerated="false">
        <outline text="metadata_version" type="string" pattern="^[1-9][0-9]*:[1-9][0-9]*:[1-9][0-9]*$" required="false" autogenerated="true" example="123:456:789" Description="Version of metadata (trackhub) [add date]"/>
        <outline text="experiment_id" type="string" required="false" autogenerated="true" example="123:456:789" Description="Global experiment id (possibly generated by TrackFind, and a carrot for making the tracks standardized)"/>
        <outline text="project_experiment_id" type="string" required="true" autogenerated="false" example="123:456:789" Description="Submitter-local identifier (within Trackhub/repo) for all object types (study, sample, experiment.. [col2])"/>
        <outline text="experiment_ontology_uri" type="string" format="uri" pattern="^(https?)://purk.obolibrary.org/obo/OBI_[0-9]+$" required="true" autogenerated="false" example="http://purl.obolibrary.org/obo/OBI_0000716" Description="EXPERIMENT_ONTOLOGY_URI" Values="OBI"/>
        <outline text="experiment_type" type="string" required="false" autogenerated="true" example="ChIP-seq assay" Description="EXPERIMENT_TYPE" Values="Enum (&quot;DNAme&quot;, &quot;RNA-Seq&quot;...)"/>
        <outline text="contact_orcid_id" type="string" format="uri" pattern="^(https?)://orchid.org/[0-9][0-9][0-9][0-9]-[0-9][0-9][0-9][0-9]-[0-9][0-9][0-9][0-9]-[0-9][0-9][0-9][0-9]$" required="true" autogenerated="false" example="https://orcid.org/0000-0002-9103-1077&#10;" Description="Contact e-mail address" Comments=" (ORCID id - Needs GDPR consent)"/>
        <outline text="target" type="string" required="true" autogenerated="false" example="MITF" Description="target (main target of dataset, e.g. antibody/disease/gene/... To be used across technologies)" Comments=" (free text, alongside experiment type. Description on what kind of content should go there, but up to each project)"/>
        <outline text="lab_protocol_description" type="string" required="false" autogenerated="false" example="Some text including date" Description="Field(s) for lab protocol metadata [should include date]"/>
        <outline text="compute_protocol_description" type="string" required="false" autogenerated="false" example="Some text including date" Description="Field(s) for computational protocol metadata [should include date]" Comments=" (We suggest to create a registry of workflows and parameter settings that one can point to - ELIXIR follow-up project?)"/>
      </outline>
      <outline text="sample" type="object" required="true" autogenerated="false">
        <outline text="biosample_id" type="string" required="true" autogenerated="false" example="SAMN08738466" Description="sample" Values="BioSampleID"/>
        <outline text="project_sample_id" type="string" required="true" autogenerated="false" example="123:456:789" Description="Submitter-local identifier (within Trackhub/repo) for all object types (study, sample, experiment.. [col2])"/>
        <outline text="sample_ontology_uri" type="string" format="uri" required="true" autogenerated="false" example="http://www.ebi.ac.uk/efo/EFO_0003042" Description="SAMPLE_ONTOLOGY_URI" Values="EFO, CL or UBERON depending on type"/>
        <outline text="sample_type" type="string" required="false" autogenerated="true" example="H1-hESC" Description="Sample type (human readable)"/>
        <outline text="biomaterial_type" type="string" required="true " autogenerated="false" example="Cell line" Description="BIOMATERIAL_TYPE" Values="Enum (&quot;Cell Line&quot;, &quot;Primary tissue&quot;...)" Comments=" (might also be supported in EDAM)"/>
      </outline>
      <outline text="study" type="object" required="true" autogenerated="false">
        <outline text="project" type="string" required="true" autogenerated="false" example="MyProject" Values="&quot;FAANG&quot;"/>
        <outline text="project_study_id" type="string" required="true" autogenerated="false" example="123:456:789" Description="Submitter-local identifier (within Trackhub/repo) for all object types (study, sample, experiment.. [col2])"/>
        <outline text="pubmed_id" type="integer" required="false" autogenerated="false" example="28459977" Description="Pubmed id (dataset / publication)"/>
        <outline text="disease_ontology_uri" type="string" format="uri" required="true" autogenerated="false" example="https://ncim.nci.nih.gov/ncimbrowser/ConceptReport.jsp?dictionary=NCI%20Metathesaurus&amp;code=C1272460" Description="DISEASE_ONTOLOGY_URI" Values="NCImetathesaurus"/>
        <outline text="disease" type="string" required="false" autogenerated="true" example="Not Applicable" Description="DISEASE" Values="String"/>
      </outline>
      <outline text="tracks" type="array" required="true" autogenerated="false">
        <outline text="track_id" type="string" required="false" autogenerated="true" example="123:456:789" Description="Global track id (generated by TrackFind?)"/>
        <outline text="project_track_id" type="string" required="true" autogenerated="false" example="123:456:789" Description="Submitter-local identifier (within Trackhub/repo) for all object types (study, sample, experiment.. [col2])"/>
        <outline text="url" type="string" format="uri" required="true" autogenerated="false" example="ftp://myproject.org/files/h1hesc-mitf.bw" Description="A URL (to track file)"/>
        <outline text="filename" type="string" required="false" autogenerated="true" example="h1hesc-mitf.bb" Values="string"/>
        <outline text="filetype_url" type="string" format="uri" required="true" autogenerated="false" example="http://edamontology.org/format_3004" Description="filetype" Values="EDAM"/>
        <outline text="filetype" type="string" required="false" autogenerated="true" example="bigBed"/>
        <outline text="checksum_method" type="string" required="true" autogenerated="false" example="MD5" Values="Enum" Comments=" (enum)"/>
        <outline text="checksum" type="string" required="true" autogenerated="false" example="79054025255fb1a26e4bc422aef54eb4" Values="string"/>
        <outline text="genome_assembly" type="string" required="true" autogenerated="false" example="hg38" Description="An assembly identifier (using UCSC nomenclature)"/>
        <outline text="short_label" type="string" required="true" autogenerated="false" example="MITF ChIP-seq (H1-hESC)" Description="A short label (string)" Comments=" (min of characters limits [14?])"/>
        <outline text="long_label" type="string" required="true" autogenerated="false" example="MITF ChIP-seq assay in human embryonic stem cell lineH1-hESC" Description="A long label (string)"/>
        <outline text="data_type_uri" type="string" format="uri" required="true " autogenerated="false" example="http://edamontology.org/data_1255" Description="data type/represented concept (e.g. narrow peaks, signal, single mutations...)" Comments="(data subtree in EDAM? Contact EDAM to improve the ontology in relation to this)"/>
        <outline text="data_type" type="string" required="false" autogenerated="true" example="Sequence features"/>
      </outline>
    </outline>
  </body>
</opml>
