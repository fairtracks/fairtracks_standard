{
    "$schema": "http://json-schema.org/draft-04/hyper-schema#", 
    "$id": "https://www.elixir-europe.org/IS/FAIRGDataTracks/json-schemas/0.1", 
    "title": "FAIRification of Genomic Data Tracks JSON Schema", 
    "type": "object", 
    "properties": {
        "fair_tracks": {
            "type": "object", 
            "properties": {
                "_schema": {
                    "description": "The JSON schema absolute URI. It must match the value of 'id' in the JSON schema", 
                    "type": "string", 
                    "format": "uri", 
                    "const": "https://www.elixir-europe.org/IS/FAIRGDataTracks/json-schemas/0.1"
                }, 
                "experiment": {
                    "type": "object", 
                    "properties": {
                        "id": {
                            "type": "object", 
                            "properties": {
                                "global_exp_id": {
                                    "description": [
                                        "Global experiment id (possibly generated by TrackFind", 
                                        "and a carrot for making the tracks standardized)"
                                    ], 
                                    "type": "string", 
                                    "examples": [
                                        "123:456:789"
                                    ]
                                }, 
                                "exp_metadata_version": {
                                    "description": "Version of metadata (trackhub) [add date]. Autogenerated by TrackFind", 
                                    "type": "string", 
                                    "pattern": "^[1-9][0-9]*:[1-9][0-9]*:[1-9][0-9]*$", 
                                    "examples": [
                                        "123:456:789"
                                    ]
                                }, 
                                "project_exp_id": {
                                    "description": "Submitter-local identifier (within Trackhub/repo) for experiment", 
                                    "type": "string", 
                                    "examples": [
                                        "123:456:789"
                                    ]
                                }
                            }, 
                            "required": [
                                "project_exp_id"
                            ]
                        }, 
                        "type": {
                            "type": "object", 
                            "properties": {
                                "exp_type_uri": {
                                    "description": "EXPERIMENT_ONTOLOGY_URI", 
                                    "type": "term", 
                                    "ontology": "http://purl.obolibrary.org/obo/obi/2018-08-27/obi.owl", 
                                    "examples": [
                                        "http://purl.obolibrary.org/obo/OBI_0000716"
                                    ]
                                }, 
                                "exp_type_term": {
                                    "description": "EXPERIMENT_TYPE. Autogenerated from untology_uri", 
                                    "type": "string", 
                                    "examples": [
                                        "ChIP-seq assay"
                                    ]
                                }
                            }, 
                            "required": [
                                "exp_type_uri"
                            ]
                        }, 
                        "target": {
                            "description": [
                                "target (main target of dataset", 
                                "e.g. antibody/disease/gene/... To be used across technologies)"
                            ], 
                            "type": "string", 
                            "examples": [
                                "MITF"
                            ]
                        }, 
                        "contact_orcid_id": {
                            "description": "Contact e-mail address", 
                            "type": "curie", 
                            "namespace": "orcid", 
                            "examples": [
                                "https://orcid.org/0000-0002-9103-1077\n"
                            ]
                        }, 
                        "lab_protocol_description": {
                            "description": "Field(s) for lab protocol metadata [should include date]", 
                            "type": "string", 
                            "examples": [
                                "Some text including date"
                            ]
                        }, 
                        "compute_protocol_description": {
                            "description": "Field(s) for computational protocol metadata [should include date]", 
                            "type": "string", 
                            "examples": [
                                "Some text including date"
                            ]
                        }
                    }, 
                    "required": [
                        "id", 
                        "type", 
                        "target", 
                        "contact_orcid_id"
                    ]
                }, 
                "sample": {
                    "type": "object", 
                    "properties": {
                        "id": {
                            "type": "object", 
                            "properties": {
                                "biosample_id": {
                                    "description": "BioSampleID", 
                                    "type": "string", 
                                    "examples": [
                                        "SAMN08738466"
                                    ]
                                }, 
                                "project_sample_id": {
                                    "description": "Submitter-local identifier (within Trackhub/repo) for sample", 
                                    "type": "string", 
                                    "examples": [
                                        "123:456:789"
                                    ]
                                }
                            }, 
                            "required": [
                                "biosample_id", 
                                "project_sample_id"
                            ]
                        }, 
                        "type": {
                            "type": "object", 
                            "properties": {
                                "sample_type_uri": {
                                    "description": [
                                        "Uri to ontology term describing sample type (EFO", 
                                        "CL or UBERON depending on type)"
                                    ], 
                                    "type": "term", 
                                    "ontology": [
                                        "http://www.ebi.ac.uk/efo/releases/2018-02-15/efo.owl", 
                                        "http://purl.obolibrary.org/obo/cl/releases/2018-07-07/cl.owl", 
                                        "http://purl.obolibrary.org/obo/uberon/releases/2018-07-30/uberon.owl"
                                    ], 
                                    "examples": [
                                        "http://www.ebi.ac.uk/efo/EFO_0003042"
                                    ]
                                }, 
                                "sample_type_term": {
                                    "description": "Sample type (human readable). Autogenerated from sample_type_uri", 
                                    "type": "string", 
                                    "examples": [
                                        "H1-hESC"
                                    ]
                                }, 
                                "biomaterial_type": {
                                    "description": "BIOMATERIAL_TYPE", 
                                    "type": "string", 
                                    "examples": [
                                        "Cell line"
                                    ]
                                }
                            }, 
                            "required": [
                                "sample_type_uri"
                            ]
                        }
                    }, 
                    "required": [
                        "id", 
                        "type"
                    ]
                }, 
                "study": {
                    "type": "object", 
                    "properties": {
                        "id": {
                            "type": "object", 
                            "properties": {
                                "project_name": {
                                    "type": "string", 
                                    "examples": [
                                        "MyProject"
                                    ]
                                }, 
                                "project_study_id": {
                                    "description": [
                                        "Submitter-local identifier (within Trackhub/repo) for all object types (study", 
                                        "sample", 
                                        "experiment.. [col2])"
                                    ], 
                                    "type": "string", 
                                    "examples": [
                                        "123:456:789"
                                    ]
                                }, 
                                "pubmed_id": {
                                    "description": "Pubmed id (dataset / publication)", 
                                    "type": "curie", 
                                    "namespace": "pubmed", 
                                    "examples": [
                                        "28459977"
                                    ]
                                }
                            }, 
                            "required": [
                                "project_name", 
                                "project_study_id"
                            ]
                        }, 
                        "phenotype": {
                            "type": "object", 
                            "properties": {
                                "disease_uri": {
                                    "description": "DISEASE_ONTOLOGY_URI", 
                                    "type": "term", 
                                    "namespace": "http://purl.obolibrary.org/obo/ncit/releases/2018-04-05/ncit.owl", 
                                    "examples": [
                                        "https://ncim.nci.nih.gov/ncimbrowser/ConceptReport.jsp?dictionary=NCI%20Metathesaurus&code=C1272460"
                                    ]
                                }, 
                                "disease_term": {
                                    "description": "DISEASE. Autogenerated from disease_uri", 
                                    "type": "string", 
                                    "examples": [
                                        "Not Applicable"
                                    ]
                                }
                            }, 
                            "required": [
                                "disease_uri"
                            ]
                        }
                    }, 
                    "required": [
                        "id", 
                        "phenotype"
                    ]
                }, 
                "tracks": {
                    "type": "array", 
                    "items": {
                        "type": "object", 
                        "properties": {
                            "id": {
                                "type": "object", 
                                "properties": {
                                    "global_track_id": {
                                        "description": "Global track id (generated by TrackFind?)", 
                                        "type": "string", 
                                        "examples": [
                                            "123:456:789"
                                        ]
                                    }, 
                                    "project_track_id": {
                                        "description": [
                                            "Submitter-local identifier (within Trackhub/repo) for all object types (study", 
                                            "sample", 
                                            "experiment.. [col2])"
                                        ], 
                                        "type": "string", 
                                        "examples": [
                                            "123:456:789"
                                        ]
                                    }
                                }, 
                                "required": [
                                    "project_track_id"
                                ]
                            }, 
                            "url": {
                                "description": "A URL (to track file)", 
                                "type": "string", 
                                "format": "uri", 
                                "examples": [
                                    "ftp://myproject.org/files/h1hesc-mitf.bw"
                                ]
                            }, 
                            "name": {
                                "type": "object", 
                                "properties": {
                                    "filename": {
                                        "type": "string", 
                                        "examples": [
                                            "h1hesc-mitf.bb"
                                        ]
                                    }, 
                                    "short_label": {
                                        "description": "A short label (string)", 
                                        "type": "string", 
                                        "examples": [
                                            "MITF ChIP-seq (H1-hESC)"
                                        ]
                                    }, 
                                    "long_label": {
                                        "description": "A long label (string)", 
                                        "type": "string", 
                                        "examples": [
                                            "MITF ChIP-seq assay in human embryonic stem cell lineH1-hESC"
                                        ]
                                    }
                                }, 
                                "required": [
                                    "short_label", 
                                    "long_label"
                                ]
                            }, 
                            "genome_assembly": {
                                "description": "An assembly identifier (using UCSC nomenclature)", 
                                "type": "string", 
                                "examples": [
                                    "hg38"
                                ]
                            }, 
                            "type": {
                                "type": "object", 
                                "properties": {
                                    "filetype_url": {
                                        "description": "filetype", 
                                        "type": "string", 
                                        "format": "uri", 
                                        "examples": [
                                            "http://edamontology.org/format_3004"
                                        ]
                                    }, 
                                    "filetype_term": {
                                        "type": "string", 
                                        "examples": [
                                            "bigBed"
                                        ]
                                    }, 
                                    "datatype_uri": {
                                        "description": [
                                            "data type/represented concept (e.g. narrow peaks", 
                                            "signal", 
                                            "single mutations...)"
                                        ], 
                                        "type": "string", 
                                        "format": "uri", 
                                        "examples": [
                                            "http://edamontology.org/data_1255"
                                        ]
                                    }, 
                                    "datatype_term": {
                                        "type": "string", 
                                        "examples": [
                                            "Sequence features"
                                        ]
                                    }
                                }, 
                                "required": [
                                    "filetype_url"
                                ]
                            }, 
                            "checksum": {
                                "type": "object", 
                                "properties": {
                                    "cs_method": {
                                        "type": "string", 
                                        "examples": [
                                            "MD5"
                                        ]
                                    }, 
                                    "cs_hash": {
                                        "type": "string", 
                                        "examples": [
                                            "79054025255fb1a26e4bc422aef54eb4"
                                        ]
                                    }
                                }, 
                                "required": [
                                    "cs_method", 
                                    "cs_hash"
                                ]
                            }
                        }, 
                        "required": [
                            "id", 
                            "url", 
                            "name", 
                            "genome_assembly", 
                            "type", 
                            "checksum"
                        ]
                    }
                }
            }, 
            "required": [
                "_schema", 
                "experiment", 
                "sample", 
                "study", 
                "tracks"
            ]
        }
    }, 
    "required": [
        "fair_tracks"
    ], 
    "additionalProperties": true, 
    "primary_key": [], 
    "dependencies": {}
}